<?xml version="1.0" encoding="UTF-8"?>
<root>
	<AnalogueModels>
		<AnalogueModel type="GliderAnalogueModel">
	    	<parameter name="carrierFrequency" type="double" value="868e+6"/>
	    	<parameter name="kappa" type="double" value="-0.04283"/>
	    	<parameter name="zeta" type="double" value="-0.1391"/>
	    </AnalogueModel>
	</AnalogueModels>
	
	
	<Decider type="GliderDecider">
		<!--Length of Start Frame Delimiter (used to compute probability of successful 
			synchronization)-->
		<parameter name="sfdLength" type="long" value="8"/>
		
		<!--minimum possible bit error rate (BER floor)-->
		<parameter name="berLowerBound" type="double" value="1e-8"/>
		
		<!--modulation type-->
		<parameter name="modulation" type="string" value="gfsk"/>
	</Decider>
</root>
